import java.util.*;

import edu.isistan.rolegame.shared.Participation;

public class ArgumentoComp extends Argumento{
	private Vector<Argumento> ArgumentosCitados;
	
	public ArgumentoComp(String nombre,Participation pers) {
		super(nombre,pers);
		ArgumentosCitados=new Vector<>();
	}
	
	public void addCita(Argumento arg){
		ArgumentosCitados.addElement(arg);
	}
	
	protected String converString() {
		String arg=new String();
		for (Argumento a: ArgumentosCitados)
			arg=arg + a.toString();
		return arg;
	}
}
